public static void convertToWave(int arr[], int n){
        
        // Your code here
    
            int t=n/2;
            int k=0;
            for(int i=0;i<t;i++)
            {
                int swap=arr[k];
                arr[k]=arr[k+1];
                arr[k+1]=swap;
                k=k+2;
            }
            
            
    }
    
